{
  "name": "nuxt-prepare",
  "type": "module",
  "version": "1.1.0",
  "packageManager": "pnpm@8.15.4",
  "description": "Run initialization steps at build time",
  "author": "Johann Schopplich <pkg@johannschopplich.com>",
  "license": "MIT",
  "homepage": "https://github.com/johannschopplich/nuxt-prepare#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/johannschopplich/nuxt-prepare.git"
  },
  "bugs": "https://github.com/johannschopplich/nuxt-prepare/issues",
  "keywords": [
    "init",
    "nuxt",
    "prepare",
    "server"
  ],
  "exports": {
    ".": {
      "types": "./dist/types.d.ts",
      "import": "./dist/module.mjs",
      "require": "./dist/module.cjs"
    },
    "./config": {
      "types": "./dist/config.d.ts",
      "import": "./dist/config.mjs",
      "require": "./dist/config.cjs"
    }
  },
  "main": "./dist/module.cjs",
  "types": "./dist/types.d.ts",
  "typesVersions": {
    "*": {
      "*": [
        "*",
        "dist/*",
        "dist/*.d.ts"
      ]
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "prepack": "nuxt-module-build build",
    "dev": "nuxi dev playground",
    "dev:build": "nuxi build playground",
    "dev:prepare": "nuxt-module-build build --stub && nuxt-module-build prepare && nuxi prepare playground",
    "docs:dev": "vitepress dev docs",
    "docs:build": "vitepress build docs",
    "docs:preview": "vitepress preview docs",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "test:types": "tsc --noEmit",
    "release": "bumpp --commit --push --tag"
  },
  "dependencies": {
    "@nuxt/kit": "^3.10.3",
    "defu": "^6.1.4",
    "jiti": "^1.21.0",
    "pathe": "^1.1.2",
    "scule": "^1.3.0",
    "type-fest": "^4.12.0"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^2.8.1",
    "@nuxt/module-builder": "^0.5.5",
    "@nuxt/schema": "^3.10.3",
    "@types/node": "^20.11.27",
    "bumpp": "^9.4.0",
    "eslint": "^8.57.0",
    "nuxt": "^3.10.3",
    "typescript": "^5.4.2",
    "vue-tsc": "^2.0.6",
    "zod": "^3.22.4"
  }
}
