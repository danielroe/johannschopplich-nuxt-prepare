{
  "name": "nuxt-prepare",
  "type": "module",
  "version": "0.1.0",
  "packageManager": "pnpm@8.6.12",
  "description": "Run initialization steps before Nuxt starts",
  "author": "Johann Schopplich <pkg@johannschopplich.com>",
  "license": "MIT",
  "homepage": "https://github.com/johannschopplich/nuxt-prepare#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/johannschopplich/nuxt-prepare.git"
  },
  "bugs": "https://github.com/johannschopplich/nuxt-prepare/issues",
  "keywords": [
    "init",
    "nuxt",
    "prepare",
    "server"
  ],
  "exports": {
    ".": {
      "types": "./dist/types.d.ts",
      "import": "./dist/module.mjs",
      "require": "./dist/module.cjs"
    },
    "./config": {
      "types": "./dist/config.d.ts",
      "import": "./dist/config.mjs",
      "require": "./dist/config.cjs"
    }
  },
  "main": "./dist/module.cjs",
  "types": "./dist/types.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "prepack": "nuxt-module-build",
    "dev": "nuxi dev playground",
    "dev:build": "nuxi build playground",
    "dev:prepare": "nuxt-module-build --stub && nuxi prepare playground",
    "docs": "pnpm -C docs run dev",
    "docs:build": "pnpm -C docs run build",
    "docs:serve": "pnpm -C docs run serve",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "test:types": "tsc --noEmit",
    "release": "bumpp --commit --push --tag",
    "prepare": "nuxi prepare playground"
  },
  "dependencies": {
    "@nuxt/kit": "^3.6.5",
    "defu": "^6.1.2",
    "jiti": "^1.19.1",
    "type-fest": "^4.2.0"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^0.40.2",
    "@nuxt/module-builder": "^0.4.0",
    "@nuxt/schema": "^3.6.5",
    "@types/node": "^20.5.0",
    "bumpp": "^9.1.1",
    "eslint": "^8.47.0",
    "nuxt": "^3.6.5",
    "nuxt-prepare": "workspace:*",
    "typescript": "^5.1.6",
    "vue-tsc": "^1.8.8",
    "zod": "^3.22.1"
  }
}
