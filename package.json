{
  "name": "nuxt-prepare",
  "type": "module",
  "version": "0.2.1",
  "packageManager": "pnpm@8.8.0",
  "description": "Run initialization steps at build time",
  "author": "Johann Schopplich <pkg@johannschopplich.com>",
  "license": "MIT",
  "homepage": "https://github.com/johannschopplich/nuxt-prepare#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/johannschopplich/nuxt-prepare.git"
  },
  "bugs": "https://github.com/johannschopplich/nuxt-prepare/issues",
  "keywords": [
    "init",
    "nuxt",
    "prepare",
    "server"
  ],
  "exports": {
    ".": {
      "types": "./dist/types.d.ts",
      "import": "./dist/module.mjs",
      "require": "./dist/module.cjs"
    },
    "./config": {
      "types": "./dist/config.d.ts",
      "import": "./dist/config.mjs",
      "require": "./dist/config.cjs"
    }
  },
  "main": "./dist/module.cjs",
  "types": "./dist/types.d.ts",
  "typesVersions": {
    "*": {
      "*": [
        "*",
        "dist/*",
        "dist/*.d.ts"
      ]
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "prepack": "nuxt-module-build",
    "dev": "nuxi dev playground",
    "dev:build": "nuxi build playground",
    "dev:prepare": "nuxt-module-build --stub && nuxi prepare playground",
    "docs": "pnpm -C docs run dev",
    "docs:build": "pnpm -C docs run build",
    "docs:serve": "pnpm -C docs run serve",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "test:types": "tsc --noEmit",
    "release": "bumpp --commit --push --tag",
    "prepare": "nuxi prepare playground"
  },
  "dependencies": {
    "@nuxt/kit": "^3.7.4",
    "defu": "^6.1.2",
    "jiti": "^1.20.0",
    "pathe": "^1.1.1",
    "scule": "^1.0.0",
    "type-fest": "^4.3.3"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^1.0.0-beta.18",
    "@nuxt/module-builder": "^0.5.2",
    "@nuxt/schema": "^3.7.4",
    "@types/node": "^20.8.2",
    "bumpp": "^9.2.0",
    "eslint": "^8.50.0",
    "nuxt": "^3.7.4",
    "nuxt-prepare": "workspace:*",
    "typescript": "^5.2.2",
    "vue-tsc": "^1.8.15",
    "zod": "^3.22.3"
  }
}
